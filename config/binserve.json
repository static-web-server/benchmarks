{
    "server": {
        "host": "127.0.0.1:1337"
    },
    "static": {
        "directory": "public",
        "served_from": "/",
        "error_pages": {
            "404": "public/404.html"
        }
    },
    "config": {
        "enable_hot_reload": false,
        "fast_mem_cache": false,
        "enable_cache_control": true,
        "enable_directory_listing": false,
        "minify_html": false,
        "follow_symlinks": false,
        "enable_logging": false
    }
}
